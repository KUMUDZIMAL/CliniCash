<script>
    document.addEventListener('DOMContentLoaded', function () {
    const form = document.getElementById('patientForm');
    const totalAmount = document.getElementById('totalAmount');
    const paidAmount = document.getElementById('paidAmount');
    const remainingAmount = document.getElementById('remainingAmount');
    const insuranceCheckbox = document.getElementById('insurance');
    const insuranceAmount = document.getElementById('insuranceAmount');

    // Function to calculate remaining amount
    function calculateRemainingAmount() {
        // Convert input values to float or use 0 if empty
        const total = parseFloat(totalAmount.value) || 0;
        const paid = parseFloat(paidAmount.value) || 0;
        // Update remaining amount field
        remainingAmount.value = (total - paid).toFixed(2);
    }

    // Add event listeners to recalculate remaining amount on input change
    totalAmount.addEventListener('input', calculateRemainingAmount);
    paidAmount.addEventListener('input', calculateRemainingAmount);

    // Add event listener to handle insurance checkbox changes
    insuranceCheckbox.addEventListener('change', function () {
        insuranceAmount.disabled = !this.checked;
    });

    // Custom form submission handling
    form.addEventListener('submit', function (e) {
        e.preventDefault();
        // Add form submission logic here
        alert('Form submitted!');
    });
});
function filterTable() {
    const input = document.getElementById('search');
    const filter = input.value.toLowerCase();
    const table = document.getElementById('insuranceTableBody');
    const rows = table.getElementsByTagName('tr');

    for (let i = 0; i < rows.length; i++) {
        const cells = rows[i].getElementsByTagName('td');
        let match = false;

        for (let j = 0; j < cells.length; j++) {
            if (cells[j]) {
                const cellValue = cells[j].textContent || cells[j].innerText;
                if (cellValue.toLowerCase().indexOf(filter) > -1) {
                    match = true;
                    break;
                }
            }
        }

        rows[i].style.display = match ? '' : 'none';
    }
}
