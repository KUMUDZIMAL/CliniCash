        <h1 class="pm-header">Supply Management System</h1>
        <form action="/supply" method="post" id="pm-form" enctype="application/x-www-form-urlencoded">
            <div class="pm-form-group">
                <label for="medical-supplies">Medical Supplies:</label>
                <input type="text" id="medical-supplies" name="medicalSupplies" placeholder="Enter medical supplies">
            </div>
            <div class="pm-form-group">
                <label for="supply-description">Supply Description:</label>
                <input type="text" id="supply-description" name="supplyDescription" placeholder="Enter description">
            </div>
            <div class="pm-form-group">
                <label for="date-of-transaction">Date of Transaction:</label>
                <input type="date" id="date-of-transaction" name="dateOfTransaction">
            </div>
            <div class="pm-form-group">
                <label for="verification-status">Verification Status:</label>
                <select id="verification-status" name="verificationStatus">
                    <option value="">Select...</option>
                    <option value="verified">Verified</option>
                    <option value="unverified">Unverified</option>
                </select>
            </div>
            <div class="pm-form-group">
                <label for="invoice-no">Invoice No.:</label>
                <input type="text" id="invoice-no" placeholder="Enter invoice number" name="invoiceNo">
            </div>
            <div class="pm-form-group">
                <label for="amount-by-suppliers">Amount By Suppliers:</label>
                <input type="number" id="amount-by-suppliers" placeholder="Enter amount" name="amountBySuppliers">
            </div>
            <div class="pm-form-group">
                <label for="department-unit">Department Unit:</label>
                <input type="text" id="department-unit" name="departmentUnit" placeholder="Enter department unit">
            </div>
            <div class="pm-form-group">
                <label for="product-suppliers">Product Suppliers:</label>
                <input type="text" id="product-suppliers" name="productSuppliers"placeholder="Enter supplier names">
            </div>
            <div class="pm-form-group">
                <label for="attachments">Invoice:</label>
                <input type="file" id="attachments" name="invoice">
            </div>
            <div class="pm-form-group">
                <label for="notes">Notes:</label>
                <input type="text" id="notes" name="notes" placeholder="Enter notes">
            </div>
            <div class="pm-button-group">
                <button type="submit">Submit</button>
            </div>
        </form>
        <div id="result" class="result"></div>
    </div>

